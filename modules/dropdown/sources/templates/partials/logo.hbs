<div class="icons">

    <span data-extra="logo"
        {{#if logo.style}}
            style="{{logo.style}}"
            class="logo"
        {{else}}
            {{#if isHistory}}
                class="logo logo-blank"
            {{else}}
                class="logo"
            {{/if}}
        {{/if}}>
        {{logo.text}}
    </span>
    <span class="micro-logo" data-extra="logo"
        {{#if logo}}style="background-color: #{{logo.backgroundColor}}"{{/if}}>
    </span>
    {{#if icon}}
        <span class="icon {{icon}}" data-extra="icon-{{icon}}"></span>
    {{/if}}
</div>
