{{#if anchorResults}}
    <div class="anchors padded">
        {{#each (take anchorResults 4)}}
            <a
                class="result"
                title="{{title}}"
                data-url="{{url}}"
                href="{{displayUrl}}"
            >
                {{title}}
            </a>
        {{/each}}
    </div>
{{/if}}
